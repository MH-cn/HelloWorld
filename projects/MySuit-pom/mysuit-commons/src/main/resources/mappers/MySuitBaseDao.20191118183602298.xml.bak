		<if test="contentFile != null">
			and content_file like #{contentFile}
		</if>
		<if test="invalidDate1 != null and invalidDate2 != null">
			and invalid_date between #{invalidDate1} and #{invalidDate2}
		</if>
		</where>
		<if test="orderParams == null">order by id desc</if>
		<if test="orderParams != null">
		<foreach collection="orderParams" item="orderParam" open="order by " separator=",">
			<if test="orderParam.name == 'id'">id ${orderParam.direction}</if>
			<if test="orderParam.name == 'advertId'">advert_id ${orderParam.direction}</if>
			<if test="orderParam.name == 'code'">code ${orderParam.direction}</if>
			<if test="orderParam.name == 'title'">title ${orderParam.direction}</if>
			<if test="orderParam.name == 'contentType'">content_type ${orderParam.direction}</if>
			<if test="orderParam.name == 'contentFile'">content_file ${orderParam.direction}</if>
			<if test="orderParam.name == 'invalidDate'">invalid_date ${orderParam.direction}</if>
		</foreach>
		</if>
		limit #{beginIndex} , #{pageSize}
